{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { 
      "src": "/_next/(.*)", 
      "dest": "/_next/$1"
    },
    { 
      "src": "/favicon.ico", 
      "dest": "/favicon.ico" 
    },
    { 
      "src": "/icons/(.*)", 
      "dest": "/icons/$1" 
    },
    { 
      "src": "/404", 
      "status": 404,
      "dest": "/404" 
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "github": {
    "silent": true
  }
} 